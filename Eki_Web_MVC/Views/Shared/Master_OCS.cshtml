
@{

}

<!DOCTYPE html>

<html lang="zh-tw">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <meta name="keywords" content="" />
    <meta name="author" content="網億數位科技" />
    <meta name="copyright" content="網億數位科技" />
    <meta name="robots" content="all" />
    <meta name="distribution" content="global" />
    <meta name="revisit-after" content="3 days" />
    <meta name="rating" content="general" />
    <meta name="description" content="" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="shortcut icon" href="~/images/cps/icons/icon-48x48.png" />

    <link href="~/lib/font-awesome/css/font-awesome.css" rel="stylesheet" />

    @Styles.Render("~/css/jquery")
    @Styles.Render("~/css/bootstrap")
    @Styles.Render("~/css/plugin")

    <link href="~/Content/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">
    <link href="~/Content/sb-admin-2.min.css" rel="stylesheet" />
    <link href="~/Content/custom/cssStyle.css" rel="stylesheet" />
    <link href="~/Content/cps/charge.css" rel="stylesheet" />
    @RenderSection("css", required: false)


</head>
<body id="page-top">
    <header id="header">
        <div class="header-base">
            <img class="logo" src="~/images/logo_ppyp.png" />
        </div>
    </header>
    <div class="wrapper">

        @RenderBody()


    </div>




    <div id="toastDiv" class="position-fixed bottom-0 right-0 p-3 bg-transparent" style="z-index:99;right:0;bottom:0;">

    </div>

    @Scripts.Render("~/js/jquery")
    @Scripts.Render("~/js/jqueryval")
    @Scripts.Render("~/js/vue")
    @Scripts.Render("~/js/bootstrap")
    @Scripts.Render("~/js/plugin")

    <script src="~/Scripts/jquery.easing.min.js"></script>
    <script src="~/Scripts/chart.js/Chart.min.js"></script>

    @*<script src="~/View_JS/Controller/Shared/Master_Empty.js?s=@File.GetLastWriteTimeUtc(Server.MapPath("~/View_JS/Controller/Shared/Master_Empty.js")).ToString("yyyyMMddHHmmss")"></script>*@

    @RenderSection("scripts", required: false)


</body>
</html>
